
sequence seq1(A) {
    C0 + A, F0 + A, C0 + A, D0 + A
}

sequence seq3() {
    C4:50<16|1>, (C3, D#3), (C3, D#3, G3), C4:50<16>
}

sequence seq4() {
    (C3, D#3, G3)<1>,       (C3, D#3, G3)<1> + 7s, 
    (C3, D#3, G3)<1>,       (C3, D#3, G3):50<1> + 7s, 
    (C3, D#3, G3)<16>,      (C3, D#3, G3)<16> + 7s, 
    (C3, D#3, G3):50<8|2>,  (C3, D#3, G3):50<8|2> + 7s
}

default: seq1($trigger)
auto: seq1(seq3())
C4: seq1(C4:50)
